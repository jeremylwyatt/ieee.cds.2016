@article{Morris2008,
	Abstract = {Many simulation environments - particularly those intended for medical
	simulation - require solid objects to deforrn at interactive rates,
	with deformation properties that correspond to real materials. Furthermore,
	new objects may be created frequently (for example, each time a new
	patient's data is processed), prohibiting manual intervention in
	the model preparation process. This paper provides a pipeline for
	rapid preparation of deformable objects with no manual intervention,
	specifically focusing on mesh generation (preparing solid meshes
