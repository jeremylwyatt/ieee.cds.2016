	Labels = {DO, grasping, integration},
	Language = {English},
	Month = {AUG},
	Note = {1999 IEEE International Symposium on Computational Intelligence in Robotics and Automation (CIRA 99), MONTEREY, CALIFORNIA, NOV 08-09, 1999},
